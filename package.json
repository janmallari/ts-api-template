{
  "name": "ts-api-template",
  "version": "0.0.1",
  "description": "TS Express API template",
  "private": true,
  "author": {
    "email": "mallari.janlester@gmail.com",
    "name": "Jan Mallari",
    "url": "https://linkedin.com/in/janmallari"
  },
  "engines": {
    "node": ">= 18"
  },
  "main": "dist/index.js",
  "scripts": {
    "up": "docker-compose -f docker-compose.yml up -d",
    "up:rebuild": "docker-compose -f docker-compose.yml up --build -d",
    "down": "docker compose -f docker-compose.yml down",
    "dev": "docker exec -it api node_modules/.bin/nodemon --config nodemon.json src/index.ts",
    "dev:tdd": "docker exec -it api node_modules/.bin/vitest --watch",
    "develop:tdd": "nodemon --config nodemon.json --exec 'vitest --watch' src/index.ts",
    "lint": "eslint . --ext .ts",
    "build": "npm run lint && resolve-tspaths && tsup --clean src/index.ts",
    "prettify": "prettier --write . -l",
    "db:migration:create": "docker exec -it api node_modules/.bin/sequelize-cli migration:generate --underscored --name",
    "db:migrate": "docker exec -it api node_modules/.bin/sequelize-cli db:migrate",
    "db:rollback": "docker exec -it api node_modules/.bin/sequelize-cli db:migrate:undo",
    "db:undo:all": "docker exec -it api node_modules/.bin/sequelize-cli db:migrate:undo:all",
    "db:seeder:create": "docker exec -it api node_modules/.bin/sequelize-cli seed:generate --name",
    "db:seed": "docker exec -it api node_modules/.bin/sequelize-cli db:seed:all",
    "db:seed:undo": "docker exec -it api node_modules/.bin/sequelize-cli db:seed:undo",
    "db:seed:undo:all": "docker exec -it api node_modules/.bin/sequelize-cli db:seed:undo:all",
    "prepare": "husky install",
    "start": "node dist/index.js",
    "test": "vitest",
    "test:ci": "vitest run",
    "coverage": "docker exec -it api node_modules/.bin/vitest --coverage",
    "release": "standard-version --prerelease mvp"
  },
  "keywords": [],
  "dependencies": {
    "@aws-sdk/client-s3": "^3.490.0",
    "@elastic/elasticsearch": "^8.12.1",
    "@mikro-orm/core": "^5.8.6",
    "@mikro-orm/mariadb": "^5.8.6",
    "@types/sequelize": "^4.28.20",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "bull": "^4.12.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "expo-server-sdk": "^3.7.0",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-fileupload": "^1.4.3",
    "helmet": "^7.0.0",
    "http-errors": "^2.0.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "mariadb": "^3.2.1",
    "morgan": "^1.10.0",
    "mysql2": "^3.6.1",
    "nodemailer": "^6.9.7",
    "sequelize": "^6.37.0",
    "sequelize-cli": "^6.6.1",
    "socket.io": "^4.7.4",
    "sqlite3": "^5.1.6",
    "standard-version": "^9.5.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.2",
    "@commitlint/config-conventional": "^17.7.0",
    "@types/bcrypt": "^5.0.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/bull": "^4.10.0",
    "@types/cors": "^2.8.14",
    "@types/elasticsearch": "^5.0.43",
    "@types/express": "^4.17.18",
    "@types/express-fileupload": "^1.4.4",
    "@types/http-errors": "^2.0.4",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/lodash": "^4.14.202",
    "@types/morgan": "^1.9.6",
    "@types/node": "^20.7.1",
    "@types/nodemailer": "^6.4.14",
    "@types/passport": "^1.0.16",
    "@types/supertest": "^2.0.16",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "@vitest/coverage-v8": "^0.34.6",
    "eslint": "^8.50.0",
    "husky": "^8.0.3",
    "nodemon": "^3.0.1",
    "prettier": "3.1.1",
    "resolve-tspaths": "^0.8.15",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "tsup": "^7.2.0",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^0.34.6"
  }
}